{
  "name": "momence-exchange-rates",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:app\"",
    "dev:api": "tsx watch proxy/proxy.ts",
    "dev:app": "vite",
    "start:proxy": "tsx proxy/proxy.ts",
    "build": "tsc -b && vite build",
    "build:proxy": "tsx --tsconfig tsconfig.proxy.json proxy/proxy.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "test:e2e": "concurrently -k -s first \"npm run dev\" \"npm run cy:run\""
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.16",
    "@vercel/node": "^5.3.13",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "node-cache": "^5.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "lint-staged": "^15.2.10",
    "recharts": "^2.13.3",
    "styled-components": "^6.1.15",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "wait-on": "^8.0.2"
  }
}
